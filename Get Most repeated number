
import java.util.Arrays;
import java.util.Map;
import java.util.stream.Collectors;

public class MaxRepeatNumber {
	
	public static void main(String[] args) {
		int[] A ={1,2,1,5,6,7,5,8,9,5};
		System.out.println(getMostRepeatedNumbers((A));
	}

	public static int getMostRepeatedNumber(int[] A){
		 Map<Integer, Integer> myMap = Arrays.stream(A).boxed().collect(Collectors.toMap(i -> i, i -> 1, Integer::sum));
		 int maximum = myMap.entrySet().stream().mapToInt(x->x.getValue()).max().getAsInt();
		 return myMap.entrySet()
			        .stream()
			        .filter(e -> e.getValue()== maximum)
			        .map(Map.Entry::getKey).findFirst().get();
			 
		 }
	 
}
